    "genkit:dev": "GOOGLE_APPLICATION_CREDENTIALS=./google-credentials.json next dev -p 9002",
    "genkit:watch": "GOOGLE_APPLICATION_CREDENTIALS=./google-credentials.json node --loader ts-node/esm --transpileOnly src/ai/dev.ts",
